	.center-block
		h1.fancy 
			span ABOUT ME
			.description
				p Ke is a passionate software developer who is fueled by the idea of designing and creating.
	.container.profile-pic
		.row
			.col-lg-12
				img(src="images/profile.jpg")
			.col-md-8
			.col-md-4
				a.btn.btn-social.btn-lg.btn-facebook.icon-customize
					i.fa.fa-facebook
				a.btn.btn-social.btn-lg.btn-linkedin.icon-customize
					i.fa.fa-linkedin
				a.btn.btn-social.btn-lg.btn-github.icon-customize(href="http://github.com/zhangkecyrus" target="_blank")
					i.fa.fa-github
				a.btn.btn-lg.icon-customize(href="mailto:zhangkecyrus@gmail.com")
					i.fa.fa-envelope
	hr			
	.center-block
		h1.fancy 
			span VENTUS
	.border
		.carousel.slide(data-ride="carousel" id="carousel1")
			ol.carousel-indicators
				li.active(data-target="#carousel1" data-slide-to="0")
				li(data-target="#carousel1" data-slide-to="1")
				li(data-target="#carousel1" data-slide-to="2")
				li(data-target="#carousel1" data-slide-to="3")
			.carousel-inner(role="listbox")
				.item.active
					img(src="images/ventus1.jpg" style="height: 100%;")
					//- .carousel-caption ...
