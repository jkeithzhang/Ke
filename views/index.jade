					//- .carousel-caption ...
				.item
					img(src="images/ventus4.jpg" style="height: 100%;")
					//- .carousel-caption ...
			a.left.carousel-control(href="#carousel1" role="button" data-slide="prev")
				span.glyphicon.glyphicon-chevron-left(aria-hidden="true")
				span.sr-only Previous
			a.right.carousel-control(href="#carousel1" role="button" data-slide="next")
				span.glyphicon.glyphicon-chevron-right(aria-hidden="true")
				span.sr-only Next
	hr
