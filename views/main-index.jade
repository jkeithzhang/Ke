							i.fa.fa-github
						a.btn.btn-lg.icon-customize(href="mailto:zhangkecyrus@gmail.com")
							i.fa.fa-envelope
			.spaceblack.row
				.col-md-12(style="padding-left:30px;")
					h2
						#trigger1.spacer.s0
						#s2.box2.skin
							span(style="opacity:0;display:block;font-weight:100;font-size:100px;word-spacing:-25px;line-height:85%;color:white;") Ke Zhang
